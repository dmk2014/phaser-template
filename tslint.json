{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "cyclomatic-complexity": [
            true,
            15
        ],
        "member-access": [
            true,
            "no-public"
        ],
        "object-literal-sort-keys": [
            false
        ],
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "arrays": "always",
                    "objects": "always",
                    "functions": "never",
                    "typeLiterals": "never",
                    "imports": "never",
                    "exports": "never"
                }
            }
        ]
    },
    "rulesDirectory": []
}